<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:screenOrientation="portrait"
    tools:context="com.geoteam.geostory.HomeScreenActivity" >
    
	 <AbsoluteLayout
	     android:id="@+id/linearLayout1"
	     android:layout_width="match_parent"
	     android:layout_height="match_parent"
	     android:layout_alignParentLeft="true"
	     android:layout_alignParentTop="true"
	     android:background="@color/green"
	     android:orientation="vertical" >

	     <Button
	         android:id="@+id/button12"
	         android:layout_width="351dp"
	         android:layout_height="371dp"
	         android:layout_x="5dp"
	         android:layout_y="90dp"
	         android:background="@drawable/buttonback"
	         android:gravity="center_vertical|center_horizontal"
	         android:textColor="#FFFFFF"
	         android:textSize="20sp" />

	     <ImageView
	         android:id="@+id/imageView1"
	         android:layout_width="399dp"
	         android:layout_height="wrap_content"
	         android:layout_x="-20dp"
	         android:layout_y="-22dp"
	         android:src="@drawable/titlebar" />

	     <TextView
	         android:id="@+id/textView1"
	         android:layout_width="93dp"
	         android:layout_height="wrap_content"
	         android:layout_x="14dp"
	         android:layout_y="142dp"
	         android:text="Proximity:"
	         android:textAppearance="?android:attr/textAppearanceMedium"
	         android:textColor="#ffffff" />

	     <TextView
	         android:id="@+id/textView2"
	         android:layout_width="wrap_content"
	         android:layout_height="wrap_content"
	         android:layout_x="100dp"
	         android:layout_y="142dp"
	         android:text="100 meters"
	         android:textAppearance="?android:attr/textAppearanceMedium"
	         android:textColor="#ffffff" />

	     <SeekBar
	         android:id="@+id/seekBar1"
	         android:layout_width="142dp"
	         android:layout_height="wrap_content"
	         android:layout_x="202dp"
	         android:layout_y="141dp"
	         android:max="500"
	         android:progress="100" />

	     <ToggleButton
	         android:id="@+id/toggleButton1"
	         android:layout_width="wrap_content"
	         android:layout_height="wrap_content"
	         android:layout_x="228dp"
	         android:layout_y="198dp"
	         android:text="ToggleButton" />

	     <TextView
	         android:id="@+id/textView3"
	         android:layout_width="wrap_content"
	         android:layout_height="wrap_content"
	         android:layout_x="20dp"
	         android:layout_y="220dp"
	         android:text="WiFi Only mode"
	         android:textAppearance="?android:attr/textAppearanceMedium"
	         android:textColor="#ffffff" />

	     <TextView
	         android:id="@+id/textView4"
	         android:layout_width="wrap_content"
	         android:layout_height="wrap_content"
	         android:layout_x="26dp"
	         android:layout_y="285dp"
	         android:text="Alerts"
	         android:textAppearance="?android:attr/textAppearanceMedium"
	         android:textColor="#ffffff" />

	     <ToggleButton
	         android:id="@+id/toggleButton2"
	         android:layout_width="wrap_content"
	         android:layout_height="wrap_content"
	         android:layout_x="226dp"
	         android:layout_y="273dp"
	         android:checked="true"
	         android:text="ToggleButton" />

	     <TextView
	         android:id="@+id/textView5"
	         android:layout_width="wrap_content"
	         android:layout_height="wrap_content"
	         android:layout_x="22dp"
	         android:layout_y="357dp"
	         android:text="Stay logged in"
	         android:textAppearance="?android:attr/textAppearanceMedium"
	         android:textColor="#ffffff" />

	     <ToggleButton
	         android:id="@+id/toggleButton3"
	         android:layout_width="wrap_content"
	         android:layout_height="wrap_content"
	         android:layout_x="226dp"
	         android:layout_y="345dp"
	         android:checked="true"
	         android:text="ToggleButton"
	         android:textOff="NO"
	         android:textOn="YES" />

	 </AbsoluteLayout>

</RelativeLayout>